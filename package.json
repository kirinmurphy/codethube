{
  "name": "codethingsdotnet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma migrate deploy && npx prisma generate && next build",
    "build:prod": "node scripts/buildToProduction.js",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx prisma migrate dev --name init && npx prisma generate",
    "seed:dev": "yarn run migrate:dev && npx prisma db seed",
    "seed:prod": "npx prisma db seed",
    "compileServerFunctions": "node scripts/compileServerFunctions.js netlify/dist"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@netlify/functions": "^2.8.1",
    "@prisma/client": "^5.15.1",
    "clsx": "^2.1.1",
    "memjs": "^1.3.2",
    "next": "15.0.0-rc.0",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "react-icons": "^5.2.1",
    "react-youtube": "^10.1.0"
  },
  "devDependencies": {
    "@types/memjs": "^1.3.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/youtube": "^0.0.50",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-plugin-react-compiler": "0.0.0-experimental-938cd9a-20240601",
    "eslint": "^8",
    "eslint-config-next": "15.0.0-rc.0",
    "netlify-cli": "^17.33.5",
    "postcss": "^8",
    "prisma": "^5.15.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "resolutions": { "string-width": "^4.2.3" }
